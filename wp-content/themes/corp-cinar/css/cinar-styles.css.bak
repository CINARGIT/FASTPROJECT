/** common **/

/** button default **/

.order_button_wrapper{
	display:flex;
	justify-content:var(--bl_button_option_object_align);
}

.order_button{
	position:relative;
	display:flex;
	align-items:center;
	justify-content:var(--bl_button_option_align);
	background-color:var(--bl_button_option_color_bg);
	color:var(--bl_button_option_color_text);
	text-transform:var(--bl_button_option_transform);
	border-color:var(--bl_button_option_border_color);
	border-bottom-width:var(--bl_button_option_border_bottom);
	border-top-width:var(--bl_button_option_border_top);
	border-left-width:var(--bl_button_option_border_left);
	border-right-width:var(--bl_button_option_border_right);
	border-style:solid;
	width:var(--bl_button_option_width);
	height:var(--bl_button_option_height);
	border-radius:var(--bl_button_option_radius);
	font-size:var(--bl_button_option_font_size);
	font-weight:var(--bl_button_option_font_weight);
	margin-top:var(--bl_button_option_margin_top);
	margin-bottom:var(--bl_button_option_margin_bottom);
	margin-right:var(--bl_button_option_margin_right);
	margin-left:var(----bl_button_option_margin_left);
	padding-bottom:var(--bl_button_option_padding_bottom);
	padding-right:var(--bl_button_option_padding_right);
	padding-top:var(--bl_button_option_padding_top);
	padding-left:var(--bl_button_option_padding_left);
	box-shadow:var(--bl_button_option_horizontal_offset) var(--bl_button_option_vertical_offset) var(--bl_button_option_blur) var(--bl_button_option_spread) var(--bl_button_option_shadow_color);
}

.order_button:hover{
	background-color:var(--bl_button_option_color_bg_hover);
	color:var(--bl_button_option_color_text_hover);
	border-color:var(--bl_button_option_border_color_hover);
}

/** input default **/


.littleform_item{
	display:flex;
	justify-content:var(--bl_input_option_object_align);
	margin-bottom:var(--bl_input_option_margin_bottom);
	margin-top:var(--bl_input_option_margin_top);
