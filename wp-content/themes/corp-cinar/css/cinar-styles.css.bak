@import url('https://fonts.googleapis.com/css2?family=Jost:ital,wght@0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,400&display=swap');

body{
	line-height:1.4;
	font-family: 'Jost', sans-serif;
}

header{
	position:relative;
	z-index:10;
}

.error-404.not-found{
	padding:30px;
	text-align:center;
}

.error-404.not-found h1{
	font-weight:700;
}

body form {
    margin-bottom: 0;
}

body .section-calc-in input[type="text"]{
	border-color:#fff;
}

p+h3{
	margin-top:25px;
	margin-bottom:10px;
}

.modal .modal-content{
	padding: 43px 80px 56px;
	text-align:center;
}

.modal .close{
	cursor:pointer;
