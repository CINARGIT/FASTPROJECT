.q_item{
	background: #FFFFFF;
	border-radius: 16px;
	padding:0px 36px 0px 36px;
	margin-bottom:16px;
	transition: all .2s ease-in-out;
}

.q_item:hover{
	transform: scale(1.01);
}

.q_item.active{
	background: #FFFFFF;
}

.common .q_item_question{
	transition: all .2s ease-in-out;
	padding:22px 48px 22px 0;
	font-weight: 500;
	font-size: 18px;
	cursor:pointer;
	line-height: 28px;
	color: #333D29;
	position:relative;
}

.common .q_item_question:before, .common .q_item_question:after {
	content: '';
	display: block;
	transition: all .2s ease-in-out;
	position: absolute;
	right: 0px;
	top: 50%;
	margin-top: -3px;
	width: 14px;
	height: 8px;
	background-repeat: no-repeat;
	background-size: 14px 8px;
	transition: all .2s ease-in-out;
}

.common .q_item_question:before{
	background-image: url(../img/load_bottom.svg);
}

.common .q_item:not(.active):hover .q_item_question:before{
	opacity:0;
}

.q_item_question:after {
	opacity:0;
}

.common .q_item:not(.active):hover .q_item_question:after{
	opacity:1;
	background-image: url(../img/load_bottom-white.svg);
}

.common .q_item:not(.active):hover .q_item_answer{
	color:#fff;
}

.common .q_item.active .q_item_question:before{
	transform: rotate(180deg); 
}


.common .q_item_answer{
	border-top:1px solid #5A9842;
	padding-top:19px;
}

.q_item_answer{
	font-size: 16px;
	line-height: 28px;
	color: var(--text-color);
	padding-bottom:10px;
	display:none;
}

.plusminus {
	position: relative;
	width: 25px;
	height: 25px;
	cursor: pointer;
  }
  
  .active .plusminus:before {
	transform: translatey(-50%) rotate(-90deg);
	opacity: 0;
  }
  .active .plusminus:after {
	transform: translatey(-50%) rotate(0);
  }
  .plusminus:before, .plusminus:after {
	content: "";
	display: block;
	background-color: #333;
	position: absolute;
	top: 50%;
	left: 0;
	transition: 0.35s;
	width: 100%;
	height: 3px;
  }
  .plusminus:before {
	transform: translatey(-50%);
  }
  .plusminus:after {
	transform: translatey(-50%) rotate(90deg);
  }

  .breadcrumbs_wrap{
	display: flex;
	padding: 40px 0px;
  }

