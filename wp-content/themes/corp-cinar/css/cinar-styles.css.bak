body textarea{
	color:var(--text-color);
	border-radius:var(--global-border-radius);
	background: #FFFFFF;
	font-size:16px;
	padding-top:24px;
	border-width:1px;
	border-style:solid;
	border: 1px solid var(--input-border-color);
	outline:none !important;
	height:168px;
	resize:none !important;
	padding-left:24px;
	padding-right:24px;
}



/** search **/

.search_show{
	height:100%;
	width:97px;
	text-transform: uppercase;
	background-color:transparent; 
	align-items:center;
	font-size: 17px; 
	font-weight: 500; 
	color:#fff;
	display:flex;
	border:0px none;
	background-color:var(--bl_header_menu_color_bg);
	justify-content:center;
	align-items:center;
	top:0;
	position:absolute;
	right:0px;
}

.search_show svg{
	margin-right:3px;
}

.search_show_button{
	display:flex;
	align-items:center;
}

.searchform.closed {
    opacity: 0;
	width: 0;
}

.searchform{
    transition: width 0.5s ease-in-out, opacity 0.3s;
    width: 100%;
    overflow: hidden;
    opacity: 1;
	position:absolute;
	right:0px;
	top:0;
	height:100%;
	display:flex;
	align-items:center;
	z-index:10;
	width:100%;
	justify-content:flex-end;
	background: linear-gradient(90deg, rgba(27, 57, 95, 0.00) 0%, var(--bl_header_menu_color_bg) 51.01%); 
}

.searchform  .searchform_in{
	align-items:center;
	display:flex;
	justify-content:flex-end;
}

.searchform  .close_search{
	margin-right:20px;
}

body .searchform input[type="text"]{
	width: 407px; 
	border-bottom:1px solid #E2EAFC;
	border-top:0px none;
	border-left:0px none;
	height:32px;
	background-color:transparent;
	color:#fff;
	padding-left:0px;
	border-right:0px none;
}

.searchform button{ 
	height:32px;
	width:97px;
	text-transform: uppercase;
	background-color:transparent; 
	align-items:center;
	font-size: 17px; 
	font-weight: 500; 
	color:#fff;
	display:flex;
	border:0px none;
	justify-content:center;
}

.searchform button svg{
	margin-right:3px;
}	


